<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nomandelivary">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NOMANDELIVARY.NOMANDELIVARY"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".AlarmActivity" />
        <activity android:name=".PasswordWrongActivity" />
        <activity android:name=".PasswordChangeActivity" />
        <activity android:name=".RegisterFinishActivity" />
        <activity android:name=".DeliveryStatusActivity" />
        <activity android:name=".OpenActivity" />
        <activity android:name=".LoginFinishActivity" />
        <activity android:name=".UserResisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>